{
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "trailingComma": "es5",
  "printWidth": 90,
  "tabWidth": 2,
  "semi": false,
  "bracketSpacing": true,
  "singleQuote": true
}